		<enabled>!eq(-3,Mai)</enabled>
		<lvalues>Sempre</lvalues>
		<lvalues>Secondo emissione</lvalues>
	</settings>
	<settings>
		<id>search_new_content</id>
		<type>list</type>
		<label>   - Esegui la ricerca di contenuti</label>
		<default>0</default>
		<enabled>!eq(-4,Nunca)</enabled>
		<lvalues>Nella cartella di ogni serie</lvalues>
		<lvalues>Tutta la biblioteca</lvalues>
	</settings>
	<settings>
		<id>clean_after_update</id>
		<type>bool</type>
		<label>   - Pulire la libreria dopo l'aggiornamento</label>
		<enabled>!eq(-5,Nunca)</enabled>
		<default>false</default>
