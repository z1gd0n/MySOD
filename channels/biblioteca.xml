			<autor>Costaplus</autor>
			<description>fix delete downlaod</description>
		</change>
	</changes>

	<include_in_global_search>disabled</include_in_global_search>

	<settings>
		<id>updatelibrary</id>
		<type>list</type>
		<label>Aggiornamento libreria:</label>
		<default>1</default>
		<visible>true</visible>
