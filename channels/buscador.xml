<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<channel>
    <id>buscador</id>
    <name>Buscador</name>

    <active>false</active>
    <adult>false</adult>
    <language>it</language>
    <include_in_global_search>disabled</include_in_global_search>

    <fanart></fanart>
    <thumbnail></thumbnail>
    <bannermenu></bannermenu>

    <update_url>https://raw.githubusercontent.com/streamondemand/plugin.video.streamondemand/master/channels/</update_url>
    <version>10</version>
    <date>23/10/2016</date>
    <changes>Update library</changes>

    <categories>
        <category></category>
    </categories>

    <settings>
        <id>multithread</id>
        <type>bool</type>
        <label>Ricerca MultiThread</label>
        <default>true</default>
        <enabled>true</enabled>
        <visible>true</visible>
    </settings>
    <settings>
        <id>result_mode</id>
        <type>list</type>
        <label>Mostra risultati:</label>
        <default>0</default>
        <enabled>true</enabled>
        <visible>true</visible>
        <lvalues>Per canale</lvalues>
        <lvalues>Tutti insieme</lvalues>
    </settings>
    <settings>
        <id>saved_searches_limit</id>
        <type>list</type>
        <label>Numero ricerce salvate:</label>
        <default>0</default>
        <enabled>true</enabled>
        <visible>true</visible>
        <lvalues>10</lvalues>
        <lvalues>20</lvalues>
        <lvalues>30</lvalues>
        <lvalues>40</lvalues>
    </settings>
    <settings>
        <id>last_search</id>
        <type>bool</type>
        <label>Ricorda ultima ricerca</label>
        <default>false</default>
        <enabled>false</enabled>
        <visible>false</visible>
    </settings>

</channel>
